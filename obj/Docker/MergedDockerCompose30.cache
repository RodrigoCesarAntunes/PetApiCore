services:
  apicorepet:
    build:
      context: C:\Users\Rodrigo\source\repos\ApiCorePet
      dockerfile: ApiCorePet/Dockerfile
      target: base
    depends_on:
    - db
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: apicorepet:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/ApiCorePet.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    links:
    - db
    ports:
    - published: 8888
      target: 80
    - target: 80
    restart: always
    volumes:
    - C:\Users\Rodrigo\source\repos\ApiCorePet\ApiCorePet:/app:rw
    - C:\Users\Rodrigo\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Rodrigo\.nuget\packages:/root/.nuget/packages:ro
  db:
    environment:
      MYSQL_DATABASE: veterinarioservice
      MYSQL_PASSWORD: VetDB-436
      MYSQL_ROOT_PASSWORD: VetDB-435
      MYSQL_USER: rodrigo
    image: mysql:8.0.12
    ports:
    - published: 3306
      target: 3306
    restart: always
    volumes:
    - datafiles:/var/lib/mysql:rw
version: '3.4'
volumes:
  datafiles: {}